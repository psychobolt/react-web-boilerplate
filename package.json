{
  "name": "react-web-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --watchAll",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:tests": "eslint src/**/__tests__/*.js",
    "lint:js": "eslint **/*.js",
    "lint:css": "stylelint ./src/**/*.js",
    "flow-typed-install": "rimraf flow-typed/npm && flow-typed update -s -i peer -p",
    "flow": "flow",
    "codecov": "codecov"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-next.2",
    "@chakra-ui/theme": "^1.0.0-next.2",
    "@emotion/core": "^10.0.28",
    "hashids": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.6.9",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "semver": "7.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "codecov": "^3.7.0",
    "customize-cra": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "flow-bin": "^0.127.0",
    "flow-typed": "^3.1.0",
    "react-app-rewired": "^2.1.6",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-webpack-plugin": "^2.1.0"
  },
  "resolutions": {
    "flow-typed/semver": "7.1.3"
  }
}
